{
  "name": "nestjs-prisma-boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "jarvis394",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev:frontend": "yarn nx run frontend:serve",
    "dev:backend": "yarn nx run backend:serve",
    "dev": "yarn nx run-many -t serve -p frontend backend",
    "build:frontend": "yarn nx run frontend:build",
    "build:backend": "yarn nx run backend:build",
    "build": "yarn nx run-many -t build",
    "lint": "yarn nx run-many -t lint",
    "format": "yarn nx run-many -t lint --fix",
    "prepare": "husky && yarn prisma migrate dev"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.10",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.3.10",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.10",
    "@prisma/client": "5.16.2",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.6.13",
    "@swc/helpers": "^0.5.12",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "nestjs-prisma": "^0.23.0",
    "next": "13.4.7",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prisma": "^5.16.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.2",
    "@nestjs/schematics": "^10.1.2",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/testing": "^10.3.10",
    "@nx/esbuild": "^19.4.3",
    "@nx/eslint-plugin": "^19.4.3",
    "@nx/express": "^19.4.3",
    "@nx/js": "^19.4.3",
    "@nx/linter": "^19.4.3",
    "@nx/nest": "^19.4.3",
    "@nx/next": "^19.4.3",
    "@nx/node": "^19.4.3",
    "@nx/react": "^19.4.3",
    "@nx/webpack": "^19.4.3",
    "@nx/workspace": "^19.4.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.13",
    "@types/jest": "29.2.4",
    "@types/node": "20.14.10",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "esbuild": "^0.23.0",
    "eslint": "^8.0.1",
    "eslint-config-next": "13.4.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^9.0.11",
    "jest": "29.3.1",
    "nx": "^19.4.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^5.5.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
