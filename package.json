  "scripts": {
    "dev:frontend": "yarn nx run frontend:serve",
    "dev:backend": "yarn nx run backend:serve",
    "dev": "yarn nx run-many -t serve -p frontend backend --output-style stream",
    "build:frontend": "NODE_ENV=production yarn nx run frontend:build",
    "build:backend": "yarn nx run backend:build",
    "build": "yarn nx run-many -t build --output-style stream",
    "lint": "yarn nx run-many -t lint --output-style stream",
    "format": "yarn nx run-many -t lint --fix --output-style stream",
    "storybook": "yarn nx run frontend:storybook",
    "prepare": "husky && yarn prisma migrate dev"
