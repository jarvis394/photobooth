{
  "name": "@valley/shared",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "swc src/* --watch --out-dir dist",
    "build": "tsc -b -v",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\""
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./*": {
      "types": "./src/*.ts",
      "import": "./dist/*.js",
      "require": "./dist/*.js"
    }
  },
  "dependencies": {
    "@nestjs/mapped-types": "*"
  },
  "devDependencies": {
    "@valley/eslint-config": "*",
    "@valley/typescript-config": "*",
    "@valley/db": "*",
    "@types/node": "^20.3.1",
    "eslint": "^8.56.11",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.5",
    "typescript": "^5.3.3"
  }
}
